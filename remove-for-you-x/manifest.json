{
  "manifest_version": 3,
  "name": "Remove For You Tab on X/Twitter",
  "version": "1.0",
  "description": "Removes the 'For You' tab from X/Twitter homepage",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}